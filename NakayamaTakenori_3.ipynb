    "%matplotlib inline\n",
    "import numpy as np\n",
    "import scipy.linalg\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "N = 1000\n",
    "ItNum = int(5/5e-4)\n",
    "x = np.random.randn(N)\n",
    "v = np.random.randn(N)\n",
    "u = np.hstack([x, v])\n",
    "\n",
    "A00 = np.zeros((N,N))\n",
    "A01 = np.eye\n",
    "a = np.random.randn(N, N)\n",
    "A10 = np.tril(a) - np.tril(a).T\n",
    "\n",
    "A10 /= N\n",
    "A11 = np.zeros((N,N))\n",
    "\n",
    "A0 = np.hstack([A00, A10])\n",
    "A1 = np.hstack([A10, A11])\n",
    "A = np.vstack([A0, A1])\n",
    "\n"
    "dt = 5e-4\n",
    "dt = 5e-4\n",
    "\n",
    "Aspring = np.eye(2 * N) - (dt * A / 2)\n",
    "Bspring = (np.eye(2 * N) - dt * A / 2) * u\n",
    "LU = scipy.linalg.lu_factor(Aspring)\n",
    "for t in np.arange(N):\n",
    "    b = Bspring[:, t]\n",
    "    x = scipy.linalg.lu_solve(LU, b)\n",
    "x1 = x\n",
    "id = np.arange(0, N)\n",
    "plt.scatter(id, x1, marker = \"o\")"
   ]
